/* 封装1：箭头 */

.arrow {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.arrow::after {
  content: "";
  width: 15rpx;
  height: 15rpx;
  border-top: 3rpx solid #ccc;
  border-right: 3rpx solid #ccc;
  transform: rotate(45deg);
  display: block;
}

/* 功能封装2：专门用于做间隔的类 */

.divider {
  height: 5rpx;
  background-color: #f1f1f1;
  /* box-shadow: 0 5rpx 5rpx rgba(0, 0, 0, 0.1) inset; */
}

.divider1 {
  height: 20rpx;
  background-color: #f1f1f1;
  box-shadow: 0 5rpx 5rpx rgb(115, 255, 255, 0.1) inset;
}

/* 功能封装3：水平垂直居中 */

.center {
  display: flex;
  justify-content: center;
  align-items: center;
}

/* 功能封装4：列表封装 */

.cell {
  font-size: 16px;
  background-color: #fff;
  margin: 50rpx;
}

.cell-item {
  padding: 20rpx 10rpx;
  margin-bottom: 20rpx;
}

.cell1 {
  font-size: 16px;
  background-color: #fff;
}

.cell-item1 {
  border-bottom: 1rpx solid #ddd;
  padding: 20rpx 30rpx;
}

/* 封装5：箭头（向上） */

.arrow-up {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.arrow-up::after {
  content: "";
  width: 15rpx;
  height: 15rpx;
  border-top: 3rpx solid #ccc;
  border-right: 3rpx solid #ccc;
  transform: rotate(-45deg);
  display: block;
}

/* 封装6：箭头（向下） */

.arrow-down {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.arrow-down::after {
  content: "";
  width: 15rpx;
  height: 15rpx;
  border-top: 3rpx solid #ccc;
  border-right: 3rpx solid #ccc;
  transform: rotate(135deg);
  display: block;
}

/* 功能封装7：加载更多 */

.loadmore {
  color: #888;
  font-size: 16px;
  line-height: 100rpx;
  text-align: center;
}

.loadmore.loading::before {
  content: '';
  width: 40rpx;
  height: 40rpx;
  margin-top: -10rpx;
  margin-right: 10rpx;
  display: inline-block;
  vertical-align: middle;
  animation: loading 1s steps(12) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

@keyframes loading {
  0% {
    transform: rotate(0deg);
  }

  100% {
    transform: rotate(-360deg);
  }
}

/*删除图标*/

.delete_right {
  width: 30rpx;
  background-color: none;
  font-size:11px;
  color: #000;
  border: 1px solid #000;
  border-radius: 28rpx;
  text-align: center;
  cursor: pointer;
}
